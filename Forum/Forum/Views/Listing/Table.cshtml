@model ListingListViewModel

<div class="container">
    <h1>(@ViewBag.CurrentViewName View)</h1>
    <table class='table'>
            
        @foreach (var listing in Model.Listings)
        {
            <tr>
                <td>@listing.ListingId</td>
                <td>@listing.Name</td>
                <td>@listing.Price.ToString("0.00 NOK")</td>
                <td>@listing.Description</td>
            </tr>
        }
    </table>
    </div>
