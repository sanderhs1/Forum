@model Listing
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"> <!--Vet ikke om dette må være her, test senere-->
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
    <script src="~/lib/jquery/jquery.js"></script>
</head>
<body>
    <div class="image-container">
        <img src="@Model.ImageUrl"/>
        </div>
    <div>
        <h1>@Model.ListingId</h1>
        <h1>
            @Model.Name
        </h1>
        <h1>
            @Model.Price
        </h1>
        <h1>
            @Model.Description
        </h1>
        <a asp-controller="Rent" asp-action="CreateRentListing" asp-route-listingId="@Model.ListingId">Rent this listing</a>

    </div>
</body>





